      "UI/*": ["components/UI/*"],
      "Pages/*": ["pages/*"],
      "Styles/*": ["styles/*"],
      "Images/*": ["public/images/*"],
      "Redux/*": ["redux/*"],
      
    }
  },
  "exclude": ["node_modules", "dist"]
}